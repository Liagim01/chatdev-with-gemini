import tkinter as tk
from datetime import datetime, timedelta
import threading
import random

class ReminderApp:
    def __init__(self, root):
        self.root = root
        self.create_gui()
        self.set_reminders()

    def create_gui(self):
        self.root.title("Reminder App")
        self.root.geometry("300x200")
        tk.Label(self.root, text="Reminder App", font=("Arial", 16)).pack(pady=20)
